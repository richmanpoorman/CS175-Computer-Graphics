<scenefile>
	<globaldata>
		<diffusecoeff v="0.5"/>
		<specularcoeff v="0.5"/>
		<ambientcoeff v="0.5"/>
	</globaldata>

	<lightdata>
		<id v="0"/>
		<color r="1" g="1" b="1"/>
		<position x="3" y="3" z="3"/>
	</lightdata>

	<lightdata>
		<id v="1"/>
		<color r="1" g="1" b="1"/>
		<position x="-3" y="3" z="3"/>
	</lightdata>

	<lightdata>
		<id v="2"/>
		<color r="1" g="1" b="1"/>
		<position x="-3" y="3" z="-3"/>
	</lightdata>

	<lightdata>
		<id v="3"/>
		<color r="1" g="1" b="1"/>
		<position x="3" y="3" z="-3"/>
	</lightdata>

    <object type="tree" name="dot">
        <transblock> 
            <scale x="0.25" y="0.01" z="0.25"/>
            <object type="primitive" name="cylinder"> 
                <diffuse r="1" g="1" b="1"/>
            </object>
        </transblock>
    </object>

    <object type="tree" name="dot-pair"> 
        <transblock> 
            <translate x="-0.25" y="0" z="0"/>
            <object type="master" name="dot"/>
        </transblock>
        <transblock> 
            <translate x="0.25" y="0" z="0"/>
            <object type="master" name="dot"/>
        </transblock>
    </object>

    <object type="tree" name="dice"> 
        
        <transblock>
            <object type="primitive" name="cube">
                <diffuse r="1" g="0" b="0"/>
            </object>
        </transblock>

        <transblock>
            <translate x="0" y="0.5" z="0"/>
            <object type="master" name="dot"/>
        </transblock>

        <transblock>
            <translate x="0" y="-0.5" z="0"/>
            <rotate x="0" y ="0" z="1" angle="180"/>
            <object type="tree">
                <transblock>
                    <object type="master" name="dot-pair"/>
                </transblock>
                <transblock>
                    <translate x="0" y="0" z="0.25"/>
                    <object type="master" name="dot-pair"/>
                </transblock>
                <transblock>
                    <translate x="0" y="0" z="-0.25"/>
                    <object type="master" name="dot-pair"/>
                </transblock>
            </object>
        </transblock>

        <transblock>
            <translate x="-0.5" y="0" z="0"/>
            <rotate x="0" y ="0" z="1" angle="90"/>
            <object type="tree">
                <transblock>
                    <translate x="0.25" y="0" z="0.25"/>
                    <object type="master" name="dot"/>
                </transblock>
                <transblock>
                    <object type="master" name="dot"/>
                </transblock>
                <transblock>
                    <translate x="-0.25" y="0" z="-0.25"/>
                    <object type="master" name="dot"/>
                </transblock>
            </object>
        </transblock>

        <transblock>
            <translate x="0.5" y="0" z="0"/>
            <rotate x="0" y ="0" z="1" angle="-90"/>
            <object type="tree">
                <transblock>
                    <translate x="0" y="0" z="0.25"/>
                    <object type="master" name="dot-pair"/>
                </transblock>
                <transblock>
                    <translate x="0" y="0" z="-0.25"/>
                    <object type="master" name="dot-pair"/>
                </transblock>
            </object>
        </transblock>

        <transblock>
            <translate x="0" y="0" z="-0.5"/>
            <rotate x="1" y ="0" z="0" angle="-90"/>
            <object type="tree">
                <transblock>
                    <translate x="0.25" y="0" z="0.25"/>
                    <object type="master" name="dot"/>
                </transblock>
                <transblock>
                    <translate x="-0.25" y="0" z="-0.25"/>
                    <object type="master" name="dot"/>
                </transblock>
            </object>
        </transblock>

        <transblock>
            <translate x="0" y="0" z="0.5"/>
            <rotate x="1" y ="0" z="0" angle="90"/>
            <object type="tree">
                <transblock>
                    <translate x="0" y="0" z="0.25"/>
                    <object type="master" name="dot-pair"/>
                </transblock>
                <transblock>
                    <translate x="0" y="0" z="-0.25"/>
                    <object type="master" name="dot-pair"/>
                </transblock>
                <transblock>
                    <object type="master" name="dot"/>
                </transblock>
            </object>
        </transblock>
    </object>

	<object type="tree" name="root">
        <transblock>
		    <object type="master" name="dice"/>
        </transblock>
        <transblock>
            <translate x="0.1" y="1" z="0.5"/>
            <rotate x="0" y ="1" z="0" angle="35"/>
		    <object type="master" name="dice"/>
        </transblock>
        <transblock>
            <translate x="-0.2" y="2" z="0.2"/>
            <rotate x="0" y="1" z="0" angle="50"/>
            <rotate x="1" y="0" z="0" angle="90"/>
		    <object type="master" name="dice"/>
        </transblock>
	</object>

</scenefile>

