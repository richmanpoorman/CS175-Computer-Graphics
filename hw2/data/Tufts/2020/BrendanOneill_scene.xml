<scenefile>
    <globaldata>
	<diffusecoeff v="0.5"/>
	<specularcoeff v="0.5"/>
	<ambientcoeff v="0.5"/>
    </globaldata>

    <cameradata>
	<pos x="2" y="2" z="2"/>
	<focus x="0" y="0" z="0"/>
	<up x="0" y="1" z="0"/>
	<heightangle v="75"/>
    </cameradata>

    <lightdata>
	<id v="0"/>
	<color r="1" g="1" b="1"/>
	<position x="4" y="4" z="4"/>
    </lightdata>

    <lightdata>
	<id v="1"/>
	<color r="1" g="1" b="1"/>
	<position x="-3" y="3" z="3"/>
    </lightdata>

    <lightdata>
	<id v="2"/>
	<color r="1" g="1" b="1"/>
	<position x="-3" y="3" z="-3"/>
    </lightdata>

    <lightdata>
	<id v="3"/>
	<color r="1" g="1" b="1"/>
	<position x="3" y="3" z="-3"/>
    </lightdata>

    <object type="tree" name="root">

    <!-- front truck and wheels -->
    <transblock>
    	<rotate x="1" y="0" z="0" angle="90"/>
    	<translate x="1" y="0" z="0"/>
    	<object type="tree">
    	<transblock>
    		<scale x=".1" y="1" z=".1"/>
    		<object type="primitive" name="cylinder" >
			<diffuse r=".1" g=".1" b=".1"/>
		    </object>
		</transblock>
		<transblock>
			<translate x="0" y=".5" z="0"/>
    		<scale x=".3" y=".15" z=".3"/>
    		<object type="primitive" name="cylinder" >
			<diffuse r="0" g="1" b="1"/>
		    </object>
		</transblock>
		<transblock>
			<translate x="0" y="-.5" z="0"/>
    		<scale x=".3" y=".15" z=".3"/>
    		<object type="primitive" name="cylinder" >
			<diffuse r="0" g="1" b="1"/>
		    </object>
		</transblock>
		</object>
	</transblock>

	<!-- rear truck and wheels -->
	<transblock>
    	<rotate x="1" y="0" z="0" angle="90"/>
    	<translate x="-1" y="0" z="0"/>
    	<object type="tree">
    	<transblock>
    		<scale x=".1" y="1" z=".1"/>
    		<object type="primitive" name="cylinder" >
			<diffuse r=".1" g=".1" b=".1"/>
		    </object>
		</transblock>
		<transblock>
			<translate x="0" y=".5" z="0"/>
    		<scale x=".3" y=".15" z=".3"/>
    		<object type="primitive" name="cylinder" >
			<diffuse r="0" g="1" b="1"/>
		    </object>
		</transblock>
		<transblock>
			<translate x="0" y="-.5" z="0"/>
    		<scale x=".3" y=".15" z=".3"/>
    		<object type="primitive" name="cylinder" >
			<diffuse r="0" g="1" b="1"/>
		    </object>
		</transblock>
		</object>
	</transblock>

	<!-- deck -->
	<transblock>
		<translate x="0" y=".25" z="0"/>
		<object type="tree">
		<transblock>
			<scale x="2.5" y=".05" z="1.25"/>
			<object type="primitive" name="cube" >
			<diffuse r="2" g=".82" b=".82"/>
			</object>
		</transblock>
		<transblock>
			<translate x="-1.45" y="0" z="0"/>
			<object type="tree">
			<transblock>
				<rotate x="0" y="0" z="1" angle="-10" />
				<scale x=".25" y=".05" z="1"/>
				<object type="primitive" name="cube" >
				<diffuse r="2" g=".82" b=".82"/>
				</object>
			</transblock>
			<transblock>
				<translate x="-.3" y=".05" z="0"/>
				<rotate x="0" y="0" z="1" angle="-15" />
				<scale x=".25" y=".05" z=".75"/>
				<object type="primitive" name="cube" >
				<diffuse r="2" g=".82" b=".82"/>
				</object>
			</transblock>
			<transblock>
				<translate x="-.6" y=".15" z="0"/>
				<rotate x="0" y="0" z="1" angle="-20" />
				<scale x=".25" y=".05" z=".5"/>
				<object type="primitive" name="cube" >
				<diffuse r="2" g=".82" b=".82"/>
				</object>
			</transblock>
			</object>
		</transblock>
		<transblock>
			<translate x="1.45" y="0" z="0"/>
			<object type="tree">
			<transblock>
				<rotate x="0" y="0" z="1" angle="10" />
				<scale x=".25" y=".05" z="1"/>
				<object type="primitive" name="cube" >
				<diffuse r="2" g=".82" b=".82"/>
				</object>
			</transblock>
			<transblock>
				<translate x=".3" y=".05" z="0"/>
				<rotate x="0" y="0" z="1" angle="15" />
				<scale x=".25" y=".05" z=".75"/>
				<object type="primitive" name="cube" >
				<diffuse r="2" g=".82" b=".82"/>
				</object>
			</transblock>
			<transblock>
				<translate x=".6" y=".15" z="0"/>
				<rotate x="0" y="0" z="1" angle="20" />
				<scale x=".25" y=".05" z=".5"/>
				<object type="primitive" name="cube" >
				<diffuse r="2" g=".82" b=".82"/>
				</object>
			</transblock>
			</object>
		</transblock>
		</object>
	</transblock>

	<!-- rail -->
	<transblock>
    	<rotate x="0" y="0" z="1" angle="90"/>
    	<rotate x="1" y="0" z="0" angle="15"/>
    	<translate x="-.05" y="0" z="0"/>
    	<object type="tree">
    	<transblock>
    		<scale x=".1" y="10" z=".1"/>
    		<object type="primitive" name="cylinder" >
			<diffuse r=".1" g=".1" b=".1"/>
		    </object>
		</transblock>
		<transblock>
			<rotate x="0" y="0" z="1" angle="90"/>
			<translate x="4" y=".5" z="0"/>
    		<scale x=".1" y="1" z=".1"/>
    		<object type="primitive" name="cylinder" >
			<diffuse r=".1" g=".1" b=".1"/>
		    </object>
		</transblock>
		<transblock>
			<rotate x="0" y="0" z="1" angle="90"/>
			<translate x="-4" y=".5" z="0"/>
    		<scale x=".1" y="1" z=".1"/>
    		<object type="primitive" name="cylinder" >
			<diffuse r=".1" g=".1" b=".1"/>
		    </object>
		</transblock>
		</object>
	</transblock>
		
	</object>

</scenefile>