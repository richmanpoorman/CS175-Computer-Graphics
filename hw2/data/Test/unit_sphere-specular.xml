<scenefile>
	<globaldata>
		<diffusecoeff v="0.5"/>
		<specularcoeff v="0.5"/>
		<ambientcoeff v="0.5"/>
	</globaldata>

	<cameradata>
	  <pos x="0" y="0" z="3"/>
	  <focus x="0" y="0" z="0"/>
	  <up x="0" y="1" z="0"/>
	  <heightangle v="60"/>
	</cameradata>
	

	<lightdata>
		<id v="0"/>
		<color r="1" g="1" b="1"/>
