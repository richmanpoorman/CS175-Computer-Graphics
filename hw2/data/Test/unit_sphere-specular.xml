	</lightdata>

	<object type="tree" name="root">
		<transblock>
			<object type="primitive" name="sphere" >
				<diffuse r="1" g="0" b="0"/>
				<specular r="1" g="1" b="1"/>
				<shininess value="35"/>
			</object>
		</transblock>
	</object>

</scenefile>

